      <pou name="_02_Clase_y_Objetos" pouType="program">
        <interface>
          <localVars>
            <variable name="Mercedes">
              <type>
                <derived name="Coche" />
              </type>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml"> tenemos la Clase Coche y la instanciamos y obtenemos los Objetos: Mercedes, Bmw y Audi.</xhtml>
              </documentation>
            </variable>
            <variable name="Bmw">
              <type>
                <derived name="Coche" />
              </type>
            </variable>
            <variable name="Audi">
              <type>
                <derived name="Coche" />
              </type>
            </variable>
            <variable name="Color">
              <type>
                <string />
              </type>
            </variable>
            <variable name="Marca">
              <type>
                <string />
              </type>
            </variable>
            <variable name="Acelerar">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="Conducir">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="Frenar">
              <type>
                <BOOL />
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">//Objeto Mercedes
//llamadas a sus métodos.
IF Acelerar THEN
	Mercedes.Acelerar();
	Acelerar := FALSE;
END_IF

IF Conducir THEN
	Mercedes.Conducir();
	Conducir := FALSE;
END_IF

IF Frenar THEN
	Mercedes.Frenar();
	Frenar := FALSE;
END_IF

//llamadas a sus propiedades.
Mercedes.Marca := 'Mercedes';
Mercedes.Color := 'Negro';
Color := Mercedes.Color;
</xhtml>
          </ST>
        </body>
