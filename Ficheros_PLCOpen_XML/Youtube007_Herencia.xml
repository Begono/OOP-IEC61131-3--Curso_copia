    <pous>
      <pou name="_07_Herencia" pouType="program">
        <interface>
          <localVars>
            <variable name="Padre">
              <type>
                <derived name="FB_Padre" />
              </type>
            </variable>
            <variable name="Hijo">
              <type>
                <derived name="FB_Hijo" />
              </type>
            </variable>
            <variable name="mot">
              <type>
                <derived name="Motor" />
              </type>
            </variable>
            <variable name="pump">
              <type>
                <derived name="Pump" />
              </type>
            </variable>
            <variable name="a">
              <type>
                <INT />
              </type>
              <initialValue>
                <simpleValue value="10" />
              </initialValue>
            </variable>
            <variable name="b">
              <type>
                <INT />
              </type>
              <initialValue>
                <simpleValue value="20" />
              </initialValue>
            </variable>
            <variable name="c">
              <type>
                <INT />
              </type>
            </variable>
            <variable name="d">
              <type>
                <INT />
              </type>
            </variable>
            <variable name="e">
              <type>
                <INT />
              </type>
            </variable>
            <variable name="f">
              <type>
                <INT />
              </type>
            </variable>
            <variable name="g">
              <type>
                <INT />
              </type>
            </variable>
            <variable name="h">
              <type>
                <INT />
              </type>
            </variable>
            <variable name="i">
              <type>
                <INT />
              </type>
            </variable>
            <variable name="vel">
              <type>
                <INT />
              </type>
              <initialValue>
                <simpleValue value="1" />
              </initialValue>
            </variable>
            <variable name="bOn">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="bStatusxOnMotor">
              <type>
                <BOOL />
              </type>
            </variable>
            <variable name="bStatusxOnPump">
              <type>
                <BOOL />
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">Padre(iIn1:=a);
padre.iPadreVar;
c:= padre.mDouble();
d:= padre.mMultiply();

//Se Heradan todos los Métodos y Atributos(Propiedades), también Hereda la SubClase (Hijo) las variables de la SuperClase (Padre).
hijo(iIn1:=a, iIn2:=b);
//el valor de la variable en el Hijo y en el Padre no son iguales, ya que lo que esta sucediendo dentro del area principal del Padre no se ejecuta en el Hijo...
//esto es debido a que el codigo del area principal no se herada del Padre al Hijo...
hijo.iPadreVar; 
e:= hijo.mDouble();
//se puede sobreescribir un metodo del Padre en el Hijo..., la Herencia posibilita la sobreescritura de los Metodos...
f:= hijo.mMultiply();
g:= hijo.mMinus(minus:=17);
h:= hijo.mSuperMultiply();

//Motor SuperClase
mot(iSpeed:= vel);
mot.iAngle;
IF bOn THEN
	mot.TurnON();
ELSE
	mot.TurnOff();
END_IF

bStatusxOnMotor:= mot.StatusxOn;

//Bomba SubClase de Motor
pump(iSpeed:= vel);
pump.iAngle;
IF bOn THEN
	pump.TurnON();
ELSE
	pump.TurnOff();
END_IF

bStatusxOnPump:= pump.StatusxOn;



</xhtml>
          </ST>
        </body>
        <addData>
          <data name="http://www.3s-software.com/plcopenxml/interfaceasplaintext" handleUnknown="implementation">
            <InterfaceAsPlainText>
              <xhtml xmlns="http://www.w3.org/1999/xhtml">PROGRAM _07_Herencia
VAR
	Padre: FB_Padre;
	Hijo : FB_Hijo;
	mot : Motor;
	pump: Pump;
	
	a:INT:=10;
	b:INT:=20;
	c,d,e,f,g,h,i:INT;
	
	vel:INT:=1;
	bOn : BOOL;
	bStatusxOnMotor : BOOL;
	bStatusxOnPump : BOOL;
END_VAR
</xhtml>
            </InterfaceAsPlainText>
          </data>
          <data name="http://www.3s-software.com/plcopenxml/objectid" handleUnknown="discard">
