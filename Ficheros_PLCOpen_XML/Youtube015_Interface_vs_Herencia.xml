VAR
	//Nivel Sensor: 
	fbNivelSensor			: FB_NivelSensor;
	nNivelAnalogSensor 		: INT := 8000;
	nScaledNivelSensor 		: INT;
	nSensorNivelMaxValue 	: INT;

	//UltraSonidos
	fbUltraSonic 			: FB_UltraSonicSensor;
	nAnalogSensor 			: INT := 8000;
